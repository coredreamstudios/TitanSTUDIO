<Window x:Class="TitanSTUDIO.Views.MainViewDI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TitanSTUDIO.Views"
        mc:Ignorable="d" FontSize="24"
        Title="TitanSTUDIO" Height="300" Width="500" Background="#111">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock x:Name="data"></TextBlock>
            <Button x:Name="GetDataButton" Padding="20 10" Click="GetDataButton_Click">Get Data</Button>
            <Button x:Name="OpenChildView" Padding="20 10" Click="OpenChildView_Click">Open Child View</Button>
        </StackPanel>
                    
    </Grid>
</Window>
